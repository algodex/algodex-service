{
    "version": "0.2.0",
    "configurations": [
    
        {
            "type": "node",
            "request": "launch",
            "name": "Broker",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/server.js",
            "env": {
                "APP_CONTEXT": "broker"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "BlockWorker",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/server.js",
            "env": {
                "APP_CONTEXT": "block-worker"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "OrderWorker",
            "skipFiles": [
