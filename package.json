{
  "name": "@algodex/service",
  "version": "1.0.1",
  "description": "Algodex Services",
  "main": "src/index.js",
  "type": "module",
  "log": {
    "file": "./.couchdb/log.txt"
  },
  "bin": {
    "dex-service": "./server.js"
  },
  "eslintConfig": {
    "globals": {
      "emit": true
    },
    "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true
    },
    "extends": [
      "google"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "rules": {}
  },
  "redisMemoryServer": {
    "version": "6.0.10",
    "debug": "1"
  },
  "scripts": {
    "dev": "dex watch",
    "test": "dex test",
    "test-unit": "jest",
    "test-lint": "eslint src/*.js src/services/*.js views/**/*.js server.js --fix",
    "wa-frontend": "snowpack dev",
    "watch-service": "nodemon server.js",
    "watch-broker": "cross-env APP_CONTEXT=broker nodemon server.js",
    "watch-worker": "cross-env APP_CONTEXT=worker nodemon server.js",
    "watch-views": "nodemon --watch views --exec \"dex push\"",
    "wa-docs": "nodemon --watch src --exec \"esdoc\"",
    "wa-test": "cross-env CI=true jest --watch",
    "prepare": "husky install"
  },
  "author": "Michael Feher",
  "license": "UNLICENSED",
  "dependencies": {
    "@sentry/node": "^6.13.3",
    "@sentry/tracing": "^6.13.3",
    "ajv": "^8.6.3",
    "algosdk": "^1.12.0",
    "bullmq": "^1.50.2",
    "commander": "^8.2.0",
    "ioredis": "^4.28.0",
    "module": "^1.2.5",
    "pino": "^7.0.5",
    "pino-sentry": "^0.9.0",
    "pouchdb-adapter-http": "^7.2.2",
    "pouchdb-browser": "^7.2.2",
    "pouchdb-core": "^7.2.2",
    "swagger-client": "^3.17.0",
    "ws": "^8.2.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.15.8",
    "@babel/plugin-syntax-top-level-await": "^7.14.5",
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.9.2",
    "@pika/plugin-build-types": "^0.9.2",
    "@pika/plugin-build-web": "^0.9.2",
    "@pika/plugin-simple-bin": "^0.9.2",
    "@pika/plugin-standard-pkg": "^0.9.2",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.3",
    "atob": "^2.1.2",
    "btoa": "^1.2.1",
    "bull": "^3.29.3",
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "devmoji": "^2.3.0",
    "dotenv": "^10.0.0",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-publish-markdown-plugin": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^8.0.1",
    "eslint-config-google": "^0.14.0",
    "husky": "^7.0.2",
    "ioredis-mock": "^5.6.0",
    "jest": "^27.2.5",
    "nodemon": "^2.0.13",
    "pino-pretty": "^7.1.0",
    "pouchdb-adapter-memory": "^7.2.2",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "semantic-release": "^17.0.7",
    "semantic-release-gitmoji": "^1.4.2",
    "snowpack": "^3.8.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/algodex/algodex-node-gradation.git"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
