{
  "name": "@algodex/service",
  "version": "1.1.0",
  "description": "Algodex Services",
  "main": "server.js",
  "scripts": {
    "clean": "rimraf built/",
    "copy-files1": "copyfiles -u 1 -e node_modules -e log ./src/**/*.html ./src/**/*.json ./src/**/*.csv built/src/",
    "copy-files2": "copyfiles -u 1 -e node_modules -e log ./views/**/*.json built/views/ ",
    "copy-files3": "cp -r integration_test built/integration_test",
    "build": "npm run clean && node_modules/typescript/bin/tsc && npm run copy-files1 && npm run copy-files2 && npm run copy-files3 && chmod -R a+rwx built/",
    "start-broker": "cross-env APP_CONTEXT=broker npm start",
    "start-worker": "cross-env APP_CONTEXT=worker npm start",
    "start-service": "cross-env APP_CONTEXT=socket npm start",
    "start-tiny": "cross-env APP_CONTEXT=tinyman npm start",
    "test-local": "jest --testPathPattern=built/ --testPathIgnorePatterns=rewards-integration && npm run lint",
    "test": "npm run test-unit && npm run lint",
    "test-unit": "jest --testPathPattern=built/ --testPathIgnorePatterns=verify-end-to-end --testPathIgnorePatterns=rewards-integration --testPathIgnorePatterns=get-planned-distributions",
    "dev-test": "jest --watch",
    "lint": "eslint src/*.js services/*.js bin/*.js views/**/*.js server.js",
    "prepare": "husky install",
    "record-rewards": "node built/bin/record-unrecorded-rewards",
    "check-types": "tsc --project jsconfig.json --maxNodeModuleJsDepth 0",
    "end-to-end-heavy": "node built/bin/run-end-to-end-test.js --mode=heavy && jest -- built/src/__tests__/verify-end",
    "end-to-end-light": "node built/bin/run-end-to-end-test.js --mode=light && jest -- built/src/__tests__/verify-end"
  },
  "author": "Michael Feher",
  "license": "UNLICENSED",
  "dependencies": {
    "@algodex/algodex-sdk": "^2.0.2",
    "@algofi/amm-v0": "^1.4.0",
    "@bull-board/express": "^3.11.1",
    "@pactfi/pactsdk": "^0.2.2",
    "algosdk": "^1.16.0",
    "axios": "^0.27.2",
    "bullmq": "^1.50.2",
    "cli-progress": "^3.11.2",
    "commander": "^8.2.0",
    "cross-fetch": "^3.1.5",
    "csv-parse": "^5.2.2",
    "devmoji": "^2.3.0",
    "dotenv": "^10.0.0",
    "express": "^4.18.1",
    "ioredis": "^4.28.0",
    "json-to-table": "^4.2.1",
    "json2html": "^0.0.8",
    "lodash.throttle": "^4.1.1",
    "nocache": "^3.0.4",
    "pouchdb-adapter-http": "^7.2.2",
    "pouchdb-core": "^7.2.2",
    "pouchdb-find": "^7.3.0",
    "pouchdb-mapreduce": "^7.3.0",
    "pouchdb-node": "^7.3.0",
    "swagger-client": "^3.17.0",
    "tableify": "^1.1.1",
    "ws": "^8.2.3"
  },
  "devDependencies": {
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.3",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.7.14",
    "add-cors-to-couchdb": "^0.0.6",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-google": "^0.14.0",
    "http-server": "^14.1.0",
    "husky": "^7.0.2",
    "ioredis-mock": "^5.6.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.13",
    "pouchdb": "^7.3.0",
    "pouchdb-adapter-memory": "^7.2.2",
    "pouchdb-erase": "^1.0.2",
    "redis-server": "^1.2.2",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.0.7",
    "semantic-release-gitmoji": "^1.4.2",
    "typescript": "^4.8.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/algodex/algodex-service.git"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
